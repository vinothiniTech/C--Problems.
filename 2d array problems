#include <stdio.h>
#include<limits.h>

int main() {
    int r, c;
    printf("enter the row and columns:");
    scanf("%d %d", &r, &c);
    int salary[r][c];
    int max1 = INT_MIN, max2 = INT_MIN;
    for (int i = 0;i < r;i++) {
        for (int j = 0;j < c;j++) {
            scanf("%d", &salary[i][j]);
            int x = salary[i][j];
            if (x > max1) {
                max2=max1;
                max1 = x;
            }
            else if(x < max1 && x > max2) {
                max2 = x;
            }
        }
    }
    printf("%d", max2);
    return 0;
}
